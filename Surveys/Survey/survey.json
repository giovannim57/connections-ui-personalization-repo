{
  "name": "Survay Customization",
  "title": "Survay Customization",
  "description": "Inserts new PrintPDF botton inside the Survey Form",
  "services": [
    "Customizer"
  ],
  "extensions": [
    {
      "payload": {
        "url": {
          "Match" : "https://apps.ce.collabserv.com/surveys/*"
        },  
        "include-files": [
          "Survey/Survey.user.js"
        ],
	      "include-repo":{
	       "name": "connections-ui-personalization-repo"
	     }
      },
      "name": "Survay Customization",
      "type": "com.ibm.customizer.ui",
      "path": "communities"
    }
  ]
}
